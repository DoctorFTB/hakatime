{
  "name": "dashboard",
  "version": "1.0.0",
  "description": "Dashboard for Hakatime",
  "main": "index.js",
  "author": "Konstantinos Sideris <siderisk@auth.gr>",
  "license": "UNLICENSE",
  "private": true,
  "devDependencies": {
    "eslint": "7.18.0",
    "parcel": "^1.12.4",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-custom-dist-structure": "1.1.14",
    "parcel-plugin-inliner": "1.0.14",
    "parcel-plugin-static-files-copy": "2.5.0",
    "prettier": "2.2.1"
  },
  "scripts": {
    "dev": "HAKA_VERSION=$(git rev-parse --short HEAD) parcel index.html --no-source-maps",
    "prod": "HAKA_VERSION=$(git rev-parse --short HEAD) parcel build index.html --detailed-report --no-source-maps",
    "lint": "eslint --ext js src",
    "fmt": "prettier --write src/**/*.js src/*.js"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "5.15.1",
    "apexcharts": "3.23.1",
    "bootstrap": "4.6.0",
    "jquery": "3.5.1",
    "litepicker": "1.5.7",
    "lodash": "4.17.20",
    "mithril": "2.0.4",
    "notyf": "^3.9.0",
    "popper.js": "1.16.1",
    "startbootstrap-sb-admin-2": "4.1.3",
    "typeface-nunito": "1.1.3"
  },
  "staticFiles": {
    "staticPath": "icons"
  },
  "customDistStructure": {
    "options": {
      "development": true
    },
    "config": {
      ".css": "css",
      ".map": "css",
      ".js": "js",
      "images": [".jpg", ".svg", ".png"]
    }
  }
}
